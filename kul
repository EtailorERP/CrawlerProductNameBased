<script>
  P.when("jQuery").execute(function(jQuery) {
      P.when("reviewsLightbox-js").execute(function() {
      
       
       var reviewImageGallery = new ReviewGallery("RDNN7GD6B34RL_gallerySection_AE6C37A4-1E27-11E5-8177-6586688EBBC5", "DESKTOP");
       reviewImageGallery.initializeForReview("RDNN7GD6B34RL");
                        
        var widgetDom = jQuery("#RDNN7GD6B34RL_imageSection_AE6C37A4-1E27-11E5-8177-6586688EBBC5");
        var thumbnails = widgetDom.find(".review-image-thumbnail"); 
            thumbnails.each( function(index) {
                jQuery(this).bind("click", function() {
                    var popover = widgetDom.find(".a-popover-trigger");
                    popover.click();
                    P.when("reviewsLightbox-ready-RDNN7GD6B34RL_gallerySection_AE6C37A4-1E27-11E5-8177-6586688EBBC5").execute(function() {
                       reviewImageGallery.render();
                       reviewImageGallery.showImageAtIndex(index);
                    });
                });
            });
      });
  });
</script>

















